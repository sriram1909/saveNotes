{
    "version": 2,
    "builds": [
      {
        "src": "./backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "./**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/backend/(.*)",
        "dest": "backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  